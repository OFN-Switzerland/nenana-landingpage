    "i18next-fetch-backend": "^6.0.0",
    "is-ip": "^5.0.1",
    "isbot": "^5.1.30",
    "leaflet": "^1.9.4",
    "leaflet-defaulticon-compatibility": "^0.1.2",
    "lit": "^3.3.1",
